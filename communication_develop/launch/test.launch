<launch>

   <arg name="use_tls" default="false" />
   <node name="mqtt_bridge" pkg="mqtt_bridge" type="mqtt_bridge_node.py" output="screen">
    <rosparam command="load" file="$(find mqtt_bridge)/config/demo3_params.yaml" />
    <rosparam if="$(arg use_tls)" command="load" ns="mqtt" file="$(find mqtt_bridge)/config/demo3_params.yaml" />
   </node>	

   <node name="communicate" pkg="communication_develop" type="1111command.py" output="screen" />  
 
   
   <!-- <node name= "amr_mqttFromserver" pkg="communication_develop" type="1112_mqtt_sub.py" output="screen" /> -->
 
   	
</launch>
